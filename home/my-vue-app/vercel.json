{
  "builds": [
    {
      "src": "my-vue-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "my-vue-app/dist/$1"
    },
    {
      "src": "/.*",
      "dest": "my-vue-app/dist/index.html"
    }
  ]
}